     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>CABDOC</PARAMETER>
       <KIND>P</KIND>
       <STEXT>HomSoft: Cabeçalhos de Documentos Eletrônicos</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
